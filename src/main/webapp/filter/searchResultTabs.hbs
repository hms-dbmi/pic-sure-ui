<div class="filter-search row">
    <ul class="nav nav-pills">
        {{#each this}}
	        <li><a href="#{{this}}" data-toggle="tab" aria-expanded="true">{{this}}</a></li>
        {{/each}}
    </ul>

    <div class="tab-content">
	    	{{#each this}}
			<div class="tab-pane" id="{{this}}"></div>
		{{/each}}
	</div>

</div>